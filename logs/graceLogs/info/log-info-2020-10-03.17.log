						os.write(b);
						os.close();
					} catch (IOException e1) {
						// TODO Auto-generated catch block
						e1.printStackTrace();
					}
				}
				if(mima.isSelected()&&login.isSelected())
				{
					try {
						autoLoginFile.createNewFile();
						OutputStream os=new FileOutputStream(autoLoginFile);
						String str="true";
						byte[] b=str.getBytes();
						os.write(b);
						os.close();
					} catch (IOException e1) {
						// TODO Auto-generated catch block
						e1.printStackTrace();
					}
				}
				else
				{
					try {
						autoLoginFile.createNewFile();
						OutputStream os=new FileOutputStream(autoLoginFile);
						String str="false";
						byte[] b=str.getBytes();
						os.write(b);
						os.close();
					} catch (IOException e1) {
						e1.printStackTrace();
					}
				}
				
				
