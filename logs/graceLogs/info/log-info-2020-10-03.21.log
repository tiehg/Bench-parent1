					in.close();
					id=new String(bn);
				} catch (Exception e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
				text1.setText(new String(id));
				text1.setForeground(Color.BLACK);

			}
			else
			{
				text1.setText(str);
				text1.setForeground(Color.gray);
			}
		}
		public void focusGained(FocusEvent e) {
			if(text1.getText().equals(str))
