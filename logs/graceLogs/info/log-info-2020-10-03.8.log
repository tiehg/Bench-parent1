		frame.addMouseMotionListener(new MouseMotionAdapter() {
			public void mouseDragged(MouseEvent e) {
				int x_screen=e.getXOnScreen();
				int y_screen=e.getYOnScreen();
				int xx=x_screen-x;
				int yy=y_screen-y;
				frame.setLocation(xx, yy);
