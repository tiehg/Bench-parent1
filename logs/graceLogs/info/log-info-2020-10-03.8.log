					text1.setForeground(Color.BLACK);
				}
			}
		
		public void focusLost(FocusEvent e) {
			if(text1.getText().equals("")) {
				text1.setEchoChar((char)(0));
				text1.setForeground(Color.gray);
				text1.setText(str);
			}
		}
	}
