		public JPasswordFielddHdandler(JPasswordField text1,JCheckBox rememberMima,String str) {
			this.text1=text1;
			this.rememberMima=rememberMima;
			this.str=str;
			if(rememberMima.isSelected())
			{
				String strMima="000";
				String selectSql1="select * from userinfo where qq=?";
