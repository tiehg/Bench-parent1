		public JTextFieldHdandler(JTextField text1,JCheckBox rememberMima,String str) {
			this.text1=text1;
			this.rememberMima=rememberMima;
			this.str=str;
			if(rememberMima.isSelected())
			{
				File file=new File("d:\\保存记住的账号.txt");
