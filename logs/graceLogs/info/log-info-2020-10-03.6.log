						OutputStream os=new FileOutputStream(autoLoginFile);
						String str="false";
						byte[] b=str.getBytes();
						os.write(b);
						os.close();
					} catch (IOException e1) {
						e1.printStackTrace();
					}
				}
				
				
				frame.dispose();
				JFrame jf=new JFrame();
				jf.setBounds(600,300,500,200);
				jf.setVisible(true);
				
