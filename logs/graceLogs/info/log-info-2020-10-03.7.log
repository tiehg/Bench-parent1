						e1.printStackTrace();
					}
				}
				
				
				frame.dispose();
				JFrame jf=new JFrame();
				jf.setBounds(600,300,500,200);
				jf.setVisible(true);
				
			}
			else
			JOptionPane.showMessageDialog(null,"账号或密码错误，请重新输入","提示消息",JOptionPane.ERROR_MESSAGE);
			//System.exit(0);
