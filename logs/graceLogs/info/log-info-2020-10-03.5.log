				e1.printStackTrace();
			}
			Date d2=new Date();
			time=(double)((d2.getTime()-d1.getTime())/1000);
			System.out.println("时间停留"+time);
		}
		if(time==1&&auto.isSelected())
			frame.dispose();
		else {
			if(time==1)//如果未选中自动登录的复选框，则改变其状态״̬
