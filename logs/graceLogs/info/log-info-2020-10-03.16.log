				text1.setEchoChar('*');
				text1.setForeground(Color.BLACK);
			}
			else
			{
				text1.setText(str);
				text1.setEchoChar((char)(0));//不设置回显
				text1.setForeground(Color.gray);
			}
		}
		public void focusGained(FocusEvent e) {
				if(text1.getText().equals(str))
